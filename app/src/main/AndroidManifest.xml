<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ndk">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NdkApplication">
        <activity android:name=".jni.JniExceptionActivity"></activity>
        <activity android:name=".jni.JniReferenceActivity" />
        <activity android:name=".jni.JniConstructorActivity" />
        <activity android:name=".jni.JniInvokeMethodActivity" />
        <activity android:name=".jni.JniAccessMethodActivity" />
        <activity android:name=".jni.JniAccessFieldActivity" />
        <activity android:name=".jni.JniReferenceTypeActivity" />
        <activity android:name=".jni.JniStringActivity" />
        <activity android:name=".jni.JniBasicTypeActivity" />
        <activity android:name=".MainActivity">

            <!-- <activity android:name=".BottomBarActivity"> -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".base.JniInvokeActivity" />
        <activity android:name=".load.JNIDynamicLoadActivity" />
    </application>

</manifest>